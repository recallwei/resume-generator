  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p check:type build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "check:spell": "npx cspell '**'",
    "check:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "check:stylelint": "npx stylelint '**/*.scss'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.9.0",
    "axios": "^1.2.1",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "monaco-editor": "^0.34.1",
    "nprogress": "0.2.0",
    "pinia": "^2.0.28",
    "splitpanes": "^3.1.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.17",
    "@types/nprogress": "^0.2.0",
    "@types/splitpanes": "^2.2.1",
    "@vicons/fa": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "naive-ui": "^2.34.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
